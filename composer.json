  "keywords": ["zpl", "php-wrapper"],
  "minimum-stability": "dev",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/markovesic/zpl",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "ext-gd": "*"
  },
